    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET CLR Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "release win8-x64",
            "program": "${workspaceRoot}\\src\\bom-tools-gui\\bin\\Release\\netcoreapp2.1\\win8-x64\\publish\\bom-tools-gui.exe",
            "args": [],
            "cwd": "${workspaceRoot}",
            "externalConsole": true,
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": ".Net Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}